<?xml version="1.0"?>
<!DOCTYPE mnx SYSTEM "../../../schemas/mnx.dtd">
<mnx
xmlns="https://github.com/w3c/mnx"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="https://github.com/w3c/mnx ../../../schemas/common1900.xsd">
    <global>
        <measure>
            <directions>
                <time signature="4/4"/>
            </directions>
        </measure>
        <measure/>
        <measure/>
        <measure/>
        <measure/>
