    <!-- optional global systemLayout definitions (?) -->
  
    <!-- all the scores in this mnx file -->
    <scores>
        <score name="Full Score">
            <!-- optional globals for the full score  -->
            <!--<globals>...</globals>-->
            
            <!-- systemLayout-definitions for the full score  -->
            <systemLayouts>
                <systemLayout id="sysLayout1">
                    <staffGroup name="Woodwind" bracket="straight" barline="true" displayName="false" >
                        <staffGroup name="Flutes" bracket="curly" barline="true" displayName="true" >
                            <staffGroupStaff parts="#fl1 #fl2" />
                            <staffGroupStaff parts="#fl3" />                    
                        </staffGroup>
                        <staffGroup name="Oboes" bracket="none" barline="true" displayName="true" >
                            <staffGroupStaff parts="#ob1 #ob2" />                   
                        </staffGroup>               
                    </staffGroup>
                    <staffGroup name="Piano" bracket="curly" barline="true" displayName="true" >
                        <staffGroupStaff parts="#piano" staff="1" />
                        <staffGroupStaff parts="#piano" staff="2" />
                    </staffGroup> 
                </systemLayout>
                <systemLayout id="sysLayout2">
                    <staffGroup name="Woodwind" bracket="straight" barline="true" displayName="false" >
                        <staffGroup name="Fl." bracket="curly" barline="true" displayName="true" >
                            <staffGroupStaff parts="#fl1" />
                            <staffGroupStaff parts="#fl2" />
                            <staffGroupStaff parts="#fl3" />                    
                        </staffGroup>
                        <staffGroup name="Ob." bracket="none" barline="true" displayName="true" >
                            <staffGroupStaff parts="#ob1 #ob2" />                   
                        </staffGroup>               
                    </staffGroup>
                    <staffGroup name="Piano" bracket="curly" barline="true" displayName="true" >
                        <staffGroupStaff parts="#piano" staff="1" />
                        <staffGroupStaff parts="#piano" staff="2" />
                    </staffGroup> 
                </systemLayout>
            </systemLayouts>
            
            <!-- scoreLayout for the full score
                Issue: I think the actual dimensions on the screen or paper should be
                controlled by the client application.
                The margins should be handled as they are in CSS:
                One value means set all margins to that value,
                Two values means set top and bottom to the first value, left and right to the second value,
                Four values are in the order top, right, bottom, left.
            -->
            <scoreLayout relWidth="500" relHeight="707" relMargins="30">
                <page>
                    <system startMeasureNumber="1" systemLayout="#sysLayout1"/>
                    <system startMeasureNumber="4" systemLayout="#sysLayout2"/>
                </page>                
            </scoreLayout>
        </score>

        <score name="Flute 1 part">
            <!-- optional globals for the flute 1 part -->
            <!--<globals>...</globals>-->
            
            <!-- systemLayout-definitions for the flute 1 part -->
            <systemLayouts>
                <systemLayout id="flute1SysLayout1">
                    <staffGroup name="Flute" bracket="none" barline="true" displayName="true" >
                        <staffGroupStaff parts="#fl1"/>
                    </staffGroup>
                </systemLayout>
                <systemLayout id="flute1SysLayout2">
                    <staffGroup name="Flute" bracket="none" barline="true" displayName="false" >
                        <staffGroupStaff parts="#fl1"/>
                    </staffGroup>                    
                </systemLayout>
            </systemLayouts>
            <!-- scoreLayout for the flute 1 part -->
            <scoreLayout relWidth="500" relHeight="707" relMargins="30">
                <page>
                    <system startMeasureNumber="1" systemLayout="#flute1SysLayout1"/>
                    <!-- all 7 bars are printed in this one system -->                </page>
            </scoreLayout>
        </score>

        <!-- etc., all the woodwind "scores" (booklets) -->
    
        <score name="Piano part">
            <!-- optional globals for the piano part -->
