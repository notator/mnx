                        <staffGroupStaff parts="#piano" staff="1" />
                        <staffGroupStaff parts="#piano" staff="2" />
                    </staffGroup>                    
                </systemLayout>
            </systemLayouts>
            <!-- scoreLayout for the piano part -->
            <scoreLayout relWidth="500" relHeight="707" relMargins="30">
                <page>
                    <system startMeasureNumber="1" systemLayout="#pianoSysLayout1"/>
                    <system startMeasureNumber="5" systemLayout="#pianoSysLayout2"/>
               </page>
            </scoreLayout>
        </score>
    </scores>
