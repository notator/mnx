<?xml version="1.0"?>
<mnx
xmlns="https://github.com/w3c/mnx"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="https://github.com/w3c/mnx ../../../../schemas/common1900.xsd">
    <global>
        <measures>
            <measure>
                <directions>
                    <time signature="4/4"/>
                </directions>
            </measure>
        </measures>
    </global>
    <part id="part">
        <measures>
            <measure barline="regular">
                <directions>
                    <clef sign="G" line="2"/>
                </directions>
                <sequence>                    
                    <event value="/2">
                        <note pitch="C4"/>
                        <note pitch="E4"/>
                        <note pitch="G4"/>
                    </event>
                    <event value="/2">
                        <rest/>
                    </event>                    
                </sequence>
            </measure>
        </measures>
    </part>
    <systemLayout id="systemLayout">
        <partStaff part="#part" />
    </systemLayout>
    <score name="score" relWidth="500" relHeight="100" relMargins="20">
        <pages>
            <page>
                <system startMeasureNumber="1" systemLayout="#systemlayout" />
            </page>
        </pages>
    </score>
</mnx>