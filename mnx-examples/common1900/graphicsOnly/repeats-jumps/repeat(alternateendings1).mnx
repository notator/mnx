<?xml version="1.0"?>
<mnx
xmlns="https://github.com/w3c/mnx"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="https://github.com/w3c/mnx ../../../../schemas/common1900.xsd">
    <global>
        <measures>
            <measure>
                <directions>
                    <time signature="4/4"/>
                    <repeat type="start"/>
                </directions>
            </measure>
            <measure>
                <directions>
                    <repeat type="end"/>
                    <ending type="start" number="1"/>
                    <ending type="stop"/>
                </directions>
            </measure>
            <measure>
                <directions>
                    <repeat type="end"/>
                    <ending type="start" number="2"/>
                    <ending type="stop"/>
                </directions>
            </measure>
            <measure>
                <directions>
                    <ending type="start" number="3"/>
                    <ending type="discontinue"/>
                </directions>
            </measure>
        </measures>    </global>
    <part id="part">
        <measures>
            <measure>
                <directions>
                    <clef sign="G" line="2"/>
                </directions>
                <sequence>
                    <event value="/1">
                        <note pitch="C5"/>
                    </event>
                </sequence>
            </measure>
            <measure>
                <sequence>
                    <event value="/1">
                        <note pitch="E5"/>
                    </event>
                </sequence>
            </measure>
            <measure>
                <sequence>
                    <event value="/1">
                        <note pitch="G4"/>
                    </event>
                </sequence>
            </measure>
            <measure>
                <sequence>
                    <event value="/1">
                        <note pitch="C5"/>
                    </event>
                </sequence>
            </measure>
        </measures>    </part>
    <systemLayout id="systemLayout">
        <partStaff part="#part" />        
    </systemLayout>
    <score name="score" relWidth="500" relHeight="100" relMargins="20">
        <pages>
            <page>
                <system startMeasureNumber="1" systemLayout="#systemlayout" />
            </page>   
        </pages>        
    </score>
</mnx>